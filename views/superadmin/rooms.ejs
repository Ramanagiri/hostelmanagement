<%- include('partials/header')-%>
    <style>
      .carousel-item {
       height : 100vh;
       min-height: 300px;
       background: no-repeat scroll center scroll;
       -webkit-background-size: cover;
       background-size: cover;
        }
        .cb{
          margin-bottom: 50px;
        }
    </style>
  </head>
  <%- include('partials/navbar')-%>
    <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="false">
      <div class="carousel-indicators">
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active cb" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" class="cb" aria-label="Slide 2"></button>
       
      </div>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="bh.jpg" class="d-block w-100 h-100" alt="...">
          <div class="carousel-caption d-none d-md-block">
            <a href="bhlist.html" class="btn btn-dark cb btn-lg " tabindex="-1" role="button" aria-disabled="true">Boys Hostel</a>
           
          </div> 
        </div>
        <div class="carousel-item">
          <img src="Hostel 1.png" class="d-block w-100 h-100" alt="...">
          <div class="carousel-caption d-none d-md-block">
            <a href="ghlist.html" class="btn btn-dark cb btn-lg " tabindex="-1" role="button" aria-disabled="true">Girls Hostel</a>
          </div>
        </div>
      </div>
      <button class="carousel-control-prev cb" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next cb" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
  </body>
</html>
